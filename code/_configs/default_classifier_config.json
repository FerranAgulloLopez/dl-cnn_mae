{
	"data": {
		"name": "MAMe",
		"version": "full",
		"metadata_directory": "../input/MAMe/",
		"images_directory": "../input/MAMe/MAMe_data_256/data_256/",
		"train_transformations": [
			{"name":  "rotation", "degrees":  30},
			{"name":  "horizontal_flip", "p":  0.5},
			{"name":  "erasing", "p":  0.5},
			{"name":  "crop", "size":  200}
		],
		"batch_size": 64
	},
	"model": {
		"name": "default_classifier",
		"transforms": {
			"preprocess": [{"name": "WI", "type": "normal", "mean": 0, "std": 0.02}]
		},
		"network": {
            "name": "Lassie9D",
            "filter_scale": 32,
			"softmax": 1
		},
		"train_info": {
			"number_epochs": 1,
			"loss_function": {
				"name": "DefaultClassifier",
				"criterion": "binary_cross_entropy"
			},
			"optimizer": {
				"name": "Adam",
				"beta_1": 0.5,
				"beta_2": 0.999,
				"learning_rate": {
					"value": 0.0001,
					"dynamic": [0]
				}
			}
		}
	},
	"manual_seed": 0,
	"device": "cpu"
}

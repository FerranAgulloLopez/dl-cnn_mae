{
	"data": {
		"name": "cat_faces",
		"resize_shape": [64, 64],
		"resize_method": "nearest",
		"split": {"train_size": 0.7, "val_size": 0.15, "test_size": 0.15},
		"batch_size": 64,
		"transforms": {
			"preprocess": [{"name": "normalize"},{"name": "shuffle"}],
			"data_augmentation": [0, ["rotate", "fliplr", "flipud"]]
		}
	},
	"model": {
		"type": "generative_model",
		"name": "vae",
		"pre": [0],
		"transforms": {
			"preprocess": ["WI"]
		},
		"description": {
			"latent_size": 512,
			"encoder": {
				"name": "Encoder64x64TwoBlocks",
				"filter_scale": 32
			},
			"decoder": {
				"name": "Decoder64x64ThreeBlocks",
				"filter_scale": 32
			}
		},
		"train_info": {
			"number_epochs": 1,
			"loss_function": {
				"name": "BetaVae",
				"beta_kdl": 0,
				"reduction": "sum",
				"recon_loss": "mse"
			},
			"optimizer": {
				"name": "Adam",
				"beta_1": 0.5,
				"beta_2": 0.999,
				"learning_rate": {
					"value": 0.0001,
					"dynamic": [0]
				}
			},
			"visualize_latent": {
				"latent_std_mean": 0,
				"latent_last": [0]
			}
		}
	},
	"manual_seed": 0,
	"device": "cpu"
}
